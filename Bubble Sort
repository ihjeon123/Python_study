a=[5,4,3,2,1,8,7,10] 
def bubble_sort(lst): 
    for j in range(len(lst)): 
        sorted = True 
        for i in range(len(lst)-1): 
            if lst[i] > lst[i+1]: 
                lst[i],lst[i+1]=lst[i+1],lst[i] 
                sorted=False 
        if sorted: 
            break 
    return lst 
print(bubble_sort(a))
